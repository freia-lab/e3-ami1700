# AMI 1700 device via web interface

record(ai, "$(P):LT$(NR)-V:sRdV") {
   field (DESC, "Probe's voltage")
   field (EGU, "V")
   field (DTYP, "stream")
   field (LOPR, "0")
   field (HOPR, "30.0")
   field (PREC, "2")
#   field (LOW, "150.0")
#   field (LOLO, "100.0")
#   field (LSV, "MINOR")
#   field (LLSV, "MAJOR")
   field (INP, "@ami.proto get_adc(0) $(PORT)")
   field (FLNK, "$(P):LT$(NR)-V-smooth:sRdV")
   field (SCAN, "$(SCAN) second")
}
record(ai, "$(P):LT$(NR)-V-smooth:sRdV") {
   field (DESC, "Smoothed probe's voltage")
   field (EGU, "V")
   field (DTYP, "Raw Soft Channel")
   field (LOPR, "0")
   field (HOPR, "100.0")
   field (SMOO, "0.95")
   field (PREC, "2")
   field (INP, "$(P):LT$(NR)-V:sRdV")
   field (FLNK, "$(P):LT$(NR)-R:sRdV")
}


