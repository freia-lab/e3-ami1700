record(mbbi,  "$(P):LT$(NR):sEGU") {
   field (DESC, "Probe's EGU")
   field (ZRVL, "0")
   field (ZRST, "%")
   field (ONVL, "1")
   field (ONST, "cm")
   field (TWVL, "2")
   field (TWST, "inch")
   field (INP, "@ami.proto get_egu $(PORT)")
   field (SCAN, "$(SCAN) second")
   field (DTYP, "stream")
}

record (sseq, "$(P):LT$(NR):sEGU-fanout") { 
   field (SCAN, "$(SCAN) second")
   field (DOL1, "$(P):LT$(NR):sEGU")
   field (LNK1, "$(P):LT$(NR)-level:sRdV.EGU PP") 
   field (DOL2, "$(P):LT$(NR):sEGU")
   field (LNK2, "$(P):LT$(NR)-level-smooth:sRdV.EGU PP") 
   field (DOL3, "$(P):LT$(NR):sEGU")
   field (LNK3, "$(P):LT$(NR)-length:sRdV.EGU PP") 
   field (DOL4, "$(P):LT$(NR):sEGU")
   field (LNK4, "$(P):LT$(NR)-length:cSetV.EGU PP") 
}

record(waveform, "$(P):LT$(NR)-Id") {
   field (DESC, "Probe's id")
   field (DTYP, "stream")
   field (INP, "@ami.proto get_id $(PORT)")
   field (FTVL, "CHAR")
   field (NELM, "100")
   field (PINI, "YES")
}

record(mbbi,  "$(P):LT$(NR):sConf") {
   field (DESC, "Controller's configuration")
   field (ZRVL, "0")
   field (ZRST, "Not for LHe")
   field (ZRSV, "MAJOR")
   field (ONVL, "1")
   field (ONST, "4.2K, ≤40 inches")
   field (TWVL, "2")
   field (TWST, "4.2K  ≤80 inches")
   field (THVL, "3")
   field (THST, "2K, ≤40 inches")
   field (FRVL, "4")
   field (FRST, "2K  ≤80 inches")
   field (INP, "@ami.proto get_conf $(PORT)")
   field (DTYP, "stream")
   field (PINI, "YES")
}

