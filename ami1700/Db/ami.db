#############################################
# Calculated data
#############################################

record(calc, "$(P):LT$(NR)-R:sRdV") {
   field (DESC, "Probe's resistance")
   field (EGU, "Ώ")
   field (PREC, "1")
   field (INPA, "$(P):LT$(NR)-I:sRdV")
   field (INPB, "$(P):LT$(NR)-V:sRdV")
   field (CALC, "A#0?1000.0*B/A:-1")
   field (LOW, "-0.5")
   field (LSV, "MAJOR")
   field (FLNK, "$(P):LT$(NR)-R-smooth:sRdV")
}

record(calc, "$(P):LT$(NR)-R-smooth:sRdV") {
   field (DESC, "Probe's resistance")
   field (EGU, "Ώ")
   field (PREC, "1")
   field (INPA, "$(P):LT$(NR)-I-smooth:sRdV")
   field (INPB, "$(P):LT$(NR)-V-smooth:sRdV")
   field (CALC, "A#0?1000.0*B/A:-1")
   field (LOW, "-0.5")
   field (LSV, "MAJOR")
}

